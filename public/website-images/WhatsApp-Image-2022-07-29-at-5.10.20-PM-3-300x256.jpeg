    <script>
        const trigger = document.getElementById('sticky_btn');
        const modalBody = document.querySelector('.modal_body');
        const closeBtn = document.querySelector('.close');
        trigger.addEventListener('click',function(){
            modalBody.style.display='block';
        });
        closeBtn.addEventListener('click',function(){
            modalBody.style.display='none';
        });

        // onclick
        function submitForm() {
            const isValid = validateForm();

            if (isValid) {
                document.getElementById('global_msg').innerHTML = "Successfully Submitted";
                document.getElementById('global_msg').style.color = "green";
                document.getElementById('formSubmit').reset();
            } else {
                document.getElementById('global_msg').innerHTML = "* Please fill all required fields";
                document.getElementById('global_msg').classList.add('error_color');
            }
        }
        function validateForm() {
            let Name = document.getElementById("fname").value.trim();
            let Phone = document.getElementById("phone").value.trim();
            let Email = document.getElementById("email").value.trim();
            let City = document.getElementById("city").value.trim();
            let isFormValid = true;
            
            if(Name === ''){
                showError('error_name','* Please Enter Your Name');
                isFormValid = false;
            }else{
                showError('error_name','');
            }
            if (Email === '') {
                showError('error_email','* Please enter email address');
                isFormValid = false;
            } else {
                showError('error_email','');
            }            
            if (City === '') {
                showError('error_city','* Please Enter Your City Name');
                isFormValid = false;
            } else {
                showError('error_city','');
            }            
            if (Phone === '') {
                showError('error_phone','* Please Enter Your Phone Number');
                isFormValid = false;
            } else {
                showError('error_phone','');
            }            
            return isFormValid;
        }

        function showError(elementId, message) {
            document.getElementById(elementId).innerHTML = message;
        }

        // onblur Name
        function myName(){
            let Name = document.getElementById('fname').value.trim();
            let Error = document.getElementById('error_name');
            let Input = document.getElementById('fname');
            if(Name === ''){
                Error.innerHTML="* Please Enter Your Name";
                Input.style.borderColor = "red";
            }else{
                Error.innerHTML="";
                Input.style.borderColor = "";
            }
        }
        // onblur phone
        function myPhone(){
            let Phone = document.getElementById('phone').value.trim();
            let Error = document.getElementById('error_phone');
            let Input = document.getElementById('phone');
            if(Phone === ''){
                Error.innerHTML="* Please Enter Your Phone";
                Input.style.borderColor = "red";
            }else{
                Error.innerHTML="";
                Input.style.borderColor = "";
            }
        }
        // onblur Email
        function myEmail(){
            let Email = document.getElementById('email').value.trim();
            let Error = document.getElementById('error_email');
            let Input = document.getElementById('email');
            if(Email === ''){
                Error.innerHTML="* Please Enter Your Email";
                Input.style.borderColor = "red";
            }else{
                Error.innerHTML="";
                Input.style.borderColor = "";
            }
        }
        // onblur City
        function myCity(){
            let City = document.getElementById('city').value.trim();
            let Error = document.getElementById('error_city');
            let Input = document.getElementById('city');
            if(City === ''){
                Error.innerHTML="* Please Enter Your City";
                Input.style.borderColor = "red";
            }else{
                Error.innerHTML="";
                Input.style.borderColor = "";
            }
        }
        
        // OnKeyUp Name
        function validateName(){
            let Name  = document.getElementById('fname').value.trim();
            let Error = document.getElementById('error_name');
            let Input = document.getElementById('fname');

            if(Name === ''){
                Error.innerHTML="* Please Enter Your Name";
                Input.style.borderColor = "red";
            } else if(!/^[a-zA-Z]+$/.test(Name)){
                Error.innerHTML="* Only Character Allowed";
                Input.style.borderColor = "red";
            } else if((Name.length < 3) || (Name.length > 20)){
                Error.innerHTML="* Name must be between 3 and 20 characters";
                Input.style.borderColor = "red";
            }else{
                Error.innerHTML="";
                Input.style.borderColor = "";
            }
        }
        // OnKeyUp Mobile Number
        function validatePhone(){
            let Phone  = document.getElementById('phone').value.trim();
            let Error = document.getElementById('error_phone');
            let Input = document.getElementById('phone');

            if(Phone === ''){
                Error.innerHTML="* Please Enter Your Phone Number";
                Input.style.borderColor = "red";
            } else if(Phone.length < 10){
                Error.innerHTML="* Please Valid Mobile Number";
                Input.style.borderColor = "red";
            }else{
                Error.innerHTML="";
                Input.style.borderColor = "";
            }
        }        
        // OnKeyUp Email
        function validateEmail(){
            let Email = document.getElementById("email").value;
            let error = document.getElementById("error_email");
            let Input = document.getElementById('email');
          
            if(Email === ""){
                error.innerHTML = "* Please enter an email address";
                Input.style.borderColor = "red";
            }else if(!isValidEmail(Email)){
                error.innerHTML = "* Please enter a valid email address";
                Input.style.borderColor = "red";
            }else{
                error.innerHTML = "";
                Input.style.borderColor = "";
            }
        }
        function isValidEmail(Email){
            let emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
            return emailRegex.test('Email')
        }
        // OnKeyUp City
        function validateCity(){
            let City  = document.getElementById('city').value.trim();
            let Error = document.getElementById('error_city');
            let Input = document.getElementById('city');

            if(City === ''){
                Error.innerHTML="* Please Enter Your City";
                Input.style.borderColor = "red";
            } else if(!/^[a-zA-Z]+$/.test(City)){
                Error.innerHTML="* Only Character Allowed";
                Input.style.borderColor = "red";
            }else{
                Error.innerHTML="";
                Input.style.borderColor = "";
            }
        }

    </script>

